page {
  background-color: #fff !important;
}
.placeholder, .action-label {
  color: #bbb;
}
.action-label {
  text-align: right;
  flex: 1;
  padding-right: 33rpx;
  font-size: 29rpx;
}
.bbr {
  border-bottom: 2rpx solid #f6f6f6;
}

.plus-icon {
  transform: scale(2);
  margin-bottom: 20rpx;
}

.item {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  -ms-flex-align: center;
  align-items: center;
  height: 104rpx;
  border-bottom: 1px solid #f6f6f6;
  padding-left: 30rpx;
  position: relative;
}

.item text {
  font-size: 28rpx;
  color: #666;
  min-width: 120rpx;
}

.item input {
  font-size: 28rpx;
  color: #333;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  -ms-flex: 1;
  flex: 1;
  height: 10rpx;
  line-height: 40rpx;
  text-align: right;
  padding-right: 30rpx;
  padding-left: 10rpx;
}

.address {
  display: block;
  width: 100%;
  height: 150rpx;
  font-size: 28rpx;
  color: #333;
  padding: 20rpx 30rpx;
  padding-right: 12rpx;
  line-height: 1.2;
}

.section-title {
  font-size: 28rpx;
  color: #666;
  padding: 30rpx 30rpx 24rpx;
  font-weight: 500;
}

.boxs {
  padding: 0 30rpx;
}

.box,
.box-wrap img {
  width: 174rpx;
  height: 174rpx;
  border: 2rpx solid #eee;
  border-radius: 2px;
  text-align: center;
  position: relative;
  color: #999;
  font-size: 28rpx;
}

.box-wrap {
  position: relative;
}

.box {
  padding-top: 40rpx;
}

.box-wrap img {
  -o-object-fit: contain;
  object-fit: contain;
}

.box p {
  font-size: 28rpx;
  color: #999;
  margin-top: 2rpx;
}

.aicon-plus {
  color: #cac8ce;
  font-size: 28rpx;
  font-weight: 600;
}

.section1 {
  padding-bottom: 30rpx;
}
.line {
  height: 20rpx;
  background-color: #f6f6f6;
}
.boxs1 {
  padding-bottom: 30rpx;
}

.boxs1 .box-wrap:first-child {
  margin-right: 40rpx;
}

.c-btn {
  font-size: 26rpx;
  color: #367af9;
  text-align: center;
  min-width: 180rpx;
  height: 52rpx;
  line-height: 50rpx;
  border-radius: 50rpx;
  border: 1px solid #367af9;
  margin-left: 10rpx;
  padding: 0 10rpx;
}

.c-btn.disb {
  border-color: #999;
  color: #999;
}

.c-btn:active {
  opacity: .8;
}

.b-tip {
  font-size: 28rpx;
  color: #666;
  text-align: center;
  padding-top: 24rpx;
}

.b-tip i {
  color: #367af9;
}

.b-tip i:active {
  opacity: .8;
  text-decoration: underline;
}

.item.arrow-right {
  padding-right: 30rpx;
}

.item-ls input {
  padding-right: 0;
}

.item-ls .aicon {
  font-size: 28rpx;
  padding: 30rpx 26rpx;
  color: #aaa;
  font-weight: 600;
}

.card-ul {
  padding: 0 28rpx 30rpx;
}

.card-ul ._li {
  width: 330rpx;
  height: 280rpx;
  border-radius: 4px;
  overflow: hidden;
  background: #f5f8ff;
  position: relative;
}

.card-ul image {
  width: 100%;
  height: 204rpx;
  -o-object-fit: contain;
     object-fit: contain;
}

.card-ul button {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  font-size: 24rpx;
  color: #fff;
  text-align: center;
  border-radius: 0;
  background: #367af9;
}

.card-ul ._li input {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
  z-index: 9;
}


/* 银行选择 */

.bank-page {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 99999;
  background: #fff;
}

.bank-page .top {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  background: #f2f2f2;
  z-index: 123;
  padding-left: 28rpx;
  height: 100rpx;
  overflow: hidden;
}

.search-btn {
  font-size: 26rpx;
  color: #456;
  line-height: 1;
  min-width: 10rpx;
  padding: 25rpx 28rpx;
}

.search {
  background: #fff;
  height: 70rpx;
  border-radius: 10rpx;
  padding: 0 15rpx;
}

.search input {
  padding: 0 15rpx;
  height: 70rpx;
  line-height: 40rpx;
  font-size: 26rpx;
  color: #333;
}

.search img {
  width: 26rpx;
  height: 26rpx;
}

.scroll-body {
  position: fixed;
  top: 100rpx;
  bottom: 0;
  width: 100%;
  -webkit-overflow-scrolling: touch;
}

.cur-index {
  position: fixed;
  top: 50%;
  left: 50%;
  width: 150rpx;
  height: 150rpx;
  line-height: 150rpx;
  border-radius: 15rpx;
  background: rgba(0, 0, 0, 0.3);
  color: #fff;
  font-size: 20rpx;
  text-align: center;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  z-index: 99999;
}

.li-title {
  font-weight: 400;
  font-size: 26rpx;
  color: #333;
  background: #f6f6f6;
  line-height: 60rpx;
  padding-left: 30rpx;
}

.list li {
  font-size: 26rpx;
  color: #666;
  line-height: 80rpx;
  padding-left: 28rpx;
  position: relative;
  overflow: hidden;
}

.list li::before {
  content: '';
  position: absolute;
  bottom: 0;
  left: 28rpx;
  width: 100%;
  border-bottom: 1px solid #eaeaea;
  -webkit-transform: scaleY(.5);
  transform: scaleY(.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
}

.list li:last-child {
  border-bottom: 0;
}

.list li:active {
  background: #eee;
}

.index-mask {
  position: fixed;
  top: 0;
  right: 0;
  width: 60rpx;
  height: 100%;
  z-index: 23;
}

.index-list {
  position: fixed;
  top: 50%;
  right: 0;
  color: #367af9;
  -webkit-transform: translateY(-45%);
          transform: translateY(-45%);
  overflow: hidden;
  z-index: 4567;
}

.index-list li {
  padding: 5rpx 0;
  width: 70rpx;
  text-align: center;
  font-size: 26rpx;
}

.input-file {
  position: absolute;
  top: 0;
  left: 0;
  width: 174rpx;
  height: 184rpx;
  z-index: 2;
  opacity: 0;
}